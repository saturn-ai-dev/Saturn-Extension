{
  "manifest_version": 3,
  "name": "Saturn AI Workspace",
  "version": "1.0.0",
  "description": "A sleek, AI-powered browser workspace with source grounding.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "geolocation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "sandbox": {
    "pages": [
      "sandbox.html"
    ]
  }
}